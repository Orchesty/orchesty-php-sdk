{
    "name": "orchesty/php-sdk",
    "description": "PHP sdk for Orchesty",
    "license": "Apache-2.0",
    "version": "3.0.1",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-oauth": "*",
        "hanaboso/commons-bundle": "^2.8",
        "hanaboso/mongo-data-grid": "^1.6",
        "league/oauth2-client": "^2.6",
        "phpoffice/phpspreadsheet": "^1.27",
        "symfony/monolog-bundle": "^v3.8",
        "symfony/yaml": "^v6.2"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.4",
        "symfony/debug-bundle": "^v6.2",
        "symfony/security-bundle": "^v6.2",
        "symfony/serializer": "^v6.2"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesPhpSdkTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
