{
    "name": "orchesty/php-sdk",
    "description": "PHP sdk for Orchesty",
    "license": "Apache-2.0",
    "version": "3.1.0",
    "require": {
        "php": "^8.2",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-oauth": "*",
        "hanaboso/commons-bundle": "^2.10",
        "hanaboso/mongo-data-grid": "^1.7",
        "league/oauth2-client": "^2.7",
        "phpoffice/phpspreadsheet": "^1.29",
        "symfony/monolog-bundle": "^3.10",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.5",
        "symfony/debug-bundle": "^7.0",
        "symfony/serializer": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesPhpSdkTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
