{
    "name": "hanaboso/pipes-php-sdk",
    "description": "PHP sdk for PipesFramework",
    "version": "1.4.1",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-oauth": "*",
        "hanaboso/commons-bundle": "^2.0",
        "hanaboso/mongo-data-grid": "~1.3.0",
        "hanaboso/rabbit-mq-bundle": "^1.4",
        "league/oauth2-client": "^2.4",
        "phpoffice/phpspreadsheet": "^1.13",
        "symfony/debug-bundle": "^5.1",
        "symfony/mailer": "^5.1",
        "symfony/monolog-bundle": "^3.5",
        "symfony/security-bundle": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1",
        "hanaboso/rest-bundle": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesPhpSdkTests\\": "tests/"
        }
    }
}
