{
    "name": "orchesty/php-sdk",
    "description": "PHP sdk for Orchesty",
    "license": "Apache-2.0",
    "version": "4.0.0",
    "require": {
        "php": "^8.4",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-oauth": "*",
        "hanaboso/commons-bundle": "^3.0",
        "hanaboso/mongo-data-grid": "^2.0",
        "league/oauth2-client": "^2.9",
        "phpoffice/phpspreadsheet": "^5.3",
        "symfony/monolog-bundle": "^4.0",
        "symfony/yaml": "^8.0"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^2.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/serializer": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesPhpSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesPhpSdkTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
